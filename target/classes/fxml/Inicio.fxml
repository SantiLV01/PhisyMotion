<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="502.0" prefWidth="900.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.InicioController">

    <ImageView fitHeight="526.0" fitWidth="900.0" layoutY="-7.0" preserveRatio="false">
        <image>
            <Image url="@../images/fondo.png" />
        </image>
    </ImageView>

    <ImageView fitHeight="452.0" fitWidth="382.0" layoutX="257.0" layoutY="38.0" preserveRatio="true">
        <image>
            <Image url="@../images/Libro.png" />
        </image>
    </ImageView>

    <ImageView fitHeight="283.0" fitWidth="288.0" layoutX="303.0" layoutY="98.0" preserveRatio="true">
        <image>
            <Image url="@../images/logo.png" />
        </image>
    </ImageView>

    <Button fx:id="btnIniciar"
            layoutX="738.0" layoutY="211.0"
            prefHeight="112.0" prefWidth="121.0"
            style="-fx-font-size: 18px;"
            text="Iniciar"
            onAction="#abrirMenuPrincipal" />

    <ImageView fitHeight="124.0" fitWidth="144.0" layoutX="736.0" layoutY="205.0" preserveRatio="true"
               mouseTransparent="true">
        <image>
            <Image url="@../images/Flecha.png" />
        </image>
    </ImageView>

</AnchorPane>
